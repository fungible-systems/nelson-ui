{
  "name": "@nelson-ui/core",
  "version": "0.0.3",
  "private": false,
  "scripts": {
    "prebuild": "pnpm typecheck && shx rm -rf ./dist",
    "build": "vite build",
    "dev:publish": "yalc publish --push",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx}\" *.js",
    "lint:prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit --target esnext"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.css.js",
  "module": "dist/index.css.js",
  "import": "dist/index.css.js",
  "types": "dist/types/index.css.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./index.css.js": {
      "main": "dist/index.css.js",
      "module": "dist/index.css.js",
      "import": "dist/index.css.js",
      "types": "dist/types/index.css.d.ts"
    },
    "./index.css": {
      "main": "dist/index.css.js",
      "module": "dist/index.css.js",
      "import": "dist/index.css.js",
      "types": "dist/types/index.css.d.ts"
    },
    "./main.css": "./dist/main.css"
  },
  "sideEffects":
  [
    "*.css.ts"
  ],
  "dependencies": {
    "@nelson-ui/theme": "0.1.0",
    "@vanilla-extract/css": "1.6.0",
    "@vanilla-extract/private": "^1.0.1",
    "@vanilla-extract/sprinkles": "1.3.0"
  },
  "prettier": "@stacks/prettier-config",
  "devDependencies": {
    "@stacks/prettier-config": "0.0.8",
    "@types/node": "16.9.6",
    "@types/tinycolor2": "1.4.3",
    "prettier": "2.4.1",
    "tinycolor2": "1.4.2",
    "ts-eager": "2.0.2",
    "typescript": "4.4.3"
  }
}
